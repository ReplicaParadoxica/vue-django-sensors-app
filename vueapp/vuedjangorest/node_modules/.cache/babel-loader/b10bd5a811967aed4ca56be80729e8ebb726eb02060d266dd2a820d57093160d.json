{"ast":null,"code":"import axios from 'axios';\nexport default {\n  name: 'SensorList',\n  data() {\n    return {\n      sensors: [],\n      sortBy: null,\n      sortDesc: false,\n      searchTerm: ''\n    };\n  },\n  computed: {\n    filteredSensors() {\n      const lowerCaseSearchTerm = this.searchTerm.toLowerCase();\n      return this.sensors.filter(sensor => sensor.name.toLowerCase().includes(lowerCaseSearchTerm));\n    }\n  },\n  methods: {\n    loadSensors() {\n      axios.get('http://localhost:8000/api/sensors/').then(response => {\n        this.sensors = response.data;\n      }).catch(error => {\n        console.log(error);\n      });\n    },\n    deleteSensor(pk) {\n      axios.delete(`http://localhost:8000/api/sensors/${pk}/`).then(response => {\n        this.loadSensors();\n      }).catch(error => {\n        console.log(error);\n      });\n    },\n    sort(column) {\n      if (this.sortBy === column) {\n        this.sortDesc = !this.sortDesc;\n      } else {\n        this.sortBy = column;\n        this.sortDesc = false;\n      }\n      this.filteredSensors.sort((a, b) => {\n        let aValue = a[this.sortBy];\n        let bValue = b[this.sortBy];\n        if (typeof aValue === 'string' && typeof bValue === 'string') {\n          aValue = aValue.toLowerCase();\n          bValue = bValue.toLowerCase();\n        }\n        if (aValue === bValue) {\n          return 0;\n        } else if (aValue < bValue) {\n          return this.sortDesc ? 1 : -1;\n        } else {\n          return this.sortDesc ? -1 : 1;\n        }\n      });\n    }\n  },\n  mounted() {\n    this.loadSensors();\n  }\n};","map":{"version":3,"names":["axios","name","data","sensors","sortBy","sortDesc","searchTerm","computed","filteredSensors","lowerCaseSearchTerm","toLowerCase","filter","sensor","includes","methods","loadSensors","get","then","response","catch","error","console","log","deleteSensor","pk","delete","sort","column","a","b","aValue","bValue","mounted"],"sources":["src/views/Table.vue"],"sourcesContent":["\r\n<template>\r\n    <div>\r\n        <h2>Sensors</h2>\r\n        <label for=\"search-input\">Search by Sensor Name:</label>\r\n        <input id=\"search-input\" type=\"text\" v-model=\"searchTerm\">\r\n        <table>\r\n            <thead>\r\n                <tr>\r\n                    <th @click=\"sort('id')\">id</th>\r\n                    <th @click=\"sort('name')\">Name</th>\r\n                    <th @click=\"sort('type')\">Type</th>\r\n                    <th @click=\"sort('variant')\">Variant</th>\r\n                    <th>Actions</th>\r\n                </tr>\r\n            </thead>\r\n            <tbody>\r\n                <tr v-for=\"(sensor, index) in filteredSensors\" :key=\"index\">\r\n                    <td>{{ sensor.id }}</td>\r\n                    <td>{{ sensor.name }}</td>\r\n                    <td>{{ sensor.type }}</td>\r\n                    <td>{{ sensor.variant }}</td>\r\n                    <td>\r\n                        <router-link :to=\"{ path: `/api/sensors/${sensor.id}/` }\" class=\"view-link\">\r\n                            View\r\n                        </router-link>\r\n                        <button @click=\"deleteSensor(sensor.id)\" class=\"delete-button\">Delete</button>\r\n                    </td>\r\n                </tr>\r\n            </tbody>\r\n        </table>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n    name: 'SensorList',\r\n    data() {\r\n        return {\r\n            sensors: [],\r\n            sortBy: null,\r\n            sortDesc: false,\r\n            searchTerm: '',\r\n        };\r\n    },\r\n    computed: {\r\n        filteredSensors() {\r\n            const lowerCaseSearchTerm = this.searchTerm.toLowerCase();\r\n            return this.sensors.filter(sensor =>\r\n                sensor.name.toLowerCase().includes(lowerCaseSearchTerm)\r\n            );\r\n        },\r\n    },\r\n    methods: {\r\n        loadSensors() {\r\n            axios\r\n                .get('http://localhost:8000/api/sensors/')\r\n                .then((response) => {\r\n                    this.sensors = response.data;\r\n                })\r\n                .catch((error) => {\r\n                    console.log(error);\r\n                });\r\n        },\r\n        deleteSensor(pk) {\r\n            axios\r\n                .delete(`http://localhost:8000/api/sensors/${pk}/`)\r\n                .then((response) => {\r\n                    this.loadSensors();\r\n                })\r\n                .catch((error) => {\r\n                    console.log(error);\r\n                });\r\n        },\r\n        sort(column) {\r\n            if (this.sortBy === column) {\r\n                this.sortDesc = !this.sortDesc;\r\n            } else {\r\n                this.sortBy = column;\r\n                this.sortDesc = false;\r\n            }\r\n\r\n            this.filteredSensors.sort((a, b) => {\r\n                let aValue = a[this.sortBy];\r\n                let bValue = b[this.sortBy];\r\n\r\n                if (typeof aValue === 'string' && typeof bValue === 'string') {\r\n                    aValue = aValue.toLowerCase();\r\n                    bValue = bValue.toLowerCase();\r\n                }\r\n\r\n                if (aValue === bValue) {\r\n                    return 0;\r\n                } else if (aValue < bValue) {\r\n                    return this.sortDesc ? 1 : -1;\r\n                } else {\r\n                    return this.sortDesc ? -1 : 1;\r\n                }\r\n            });\r\n        },\r\n    },\r\n    mounted() {\r\n        this.loadSensors();\r\n    },\r\n};\r\n\r\n</script>\r\n  \r\n\r\n\r\n<style scoped>\r\n/* Add your styles here */\r\nh2 {\r\n    font-size: 24px;\r\n    font-weight: bold;\r\n    margin-bottom: 16px;\r\n}\r\n\r\ntable {\r\n    width: 100%;\r\n    border-collapse: collapse;\r\n}\r\n\r\nth,\r\ntd {\r\n    padding: 8px;\r\n    text-align: left;\r\n    border: 1px solid #ccc;\r\n}\r\n\r\ntr:nth-child(even) {\r\n    background-color: #f9f9f9;\r\n}\r\n\r\ntr:hover {\r\n    background-color: #f2f2f2;\r\n}\r\n\r\n.view-link,\r\n.delete-button {\r\n    margin-right: 8px;\r\n    cursor: pointer;\r\n    color: #007bff;\r\n    text-decoration: none;\r\n}\r\n\r\n.view-link:hover,\r\n.delete-button:hover {\r\n    color: #0056b3;\r\n}\r\n</style>\r\n"],"mappings":"AAmCA,OAAAA,KAAA;AAEA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,OAAA;MACAC,MAAA;MACAC,QAAA;MACAC,UAAA;IACA;EACA;EACAC,QAAA;IACAC,gBAAA;MACA,MAAAC,mBAAA,QAAAH,UAAA,CAAAI,WAAA;MACA,YAAAP,OAAA,CAAAQ,MAAA,CAAAC,MAAA,IACAA,MAAA,CAAAX,IAAA,CAAAS,WAAA,GAAAG,QAAA,CAAAJ,mBAAA,EACA;IACA;EACA;EACAK,OAAA;IACAC,YAAA;MACAf,KAAA,CACAgB,GAAA,uCACAC,IAAA,CAAAC,QAAA;QACA,KAAAf,OAAA,GAAAe,QAAA,CAAAhB,IAAA;MACA,GACAiB,KAAA,CAAAC,KAAA;QACAC,OAAA,CAAAC,GAAA,CAAAF,KAAA;MACA;IACA;IACAG,aAAAC,EAAA;MACAxB,KAAA,CACAyB,MAAA,sCAAAD,EAAA,KACAP,IAAA,CAAAC,QAAA;QACA,KAAAH,WAAA;MACA,GACAI,KAAA,CAAAC,KAAA;QACAC,OAAA,CAAAC,GAAA,CAAAF,KAAA;MACA;IACA;IACAM,KAAAC,MAAA;MACA,SAAAvB,MAAA,KAAAuB,MAAA;QACA,KAAAtB,QAAA,SAAAA,QAAA;MACA;QACA,KAAAD,MAAA,GAAAuB,MAAA;QACA,KAAAtB,QAAA;MACA;MAEA,KAAAG,eAAA,CAAAkB,IAAA,EAAAE,CAAA,EAAAC,CAAA;QACA,IAAAC,MAAA,GAAAF,CAAA,MAAAxB,MAAA;QACA,IAAA2B,MAAA,GAAAF,CAAA,MAAAzB,MAAA;QAEA,WAAA0B,MAAA,wBAAAC,MAAA;UACAD,MAAA,GAAAA,MAAA,CAAApB,WAAA;UACAqB,MAAA,GAAAA,MAAA,CAAArB,WAAA;QACA;QAEA,IAAAoB,MAAA,KAAAC,MAAA;UACA;QACA,WAAAD,MAAA,GAAAC,MAAA;UACA,YAAA1B,QAAA;QACA;UACA,YAAAA,QAAA;QACA;MACA;IACA;EACA;EACA2B,QAAA;IACA,KAAAjB,WAAA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}