{"ast":null,"code":"import axios from 'axios';\nexport default {\n  name: 'SensorList',\n  data() {\n    return {\n      sensors: [],\n      sortBy: null,\n      sortDesc: false,\n      searchText: '',\n      showId: true,\n      showName: true,\n      showType: true,\n      showVariant: true\n    };\n  },\n  methods: {\n    loadSensors() {\n      axios.get('http://localhost:8000/api/sensors/').then(response => {\n        this.sensors = response.data;\n      }).catch(error => {\n        console.log(error);\n      });\n    },\n    sort(column) {\n      if (this.sortBy === column) {\n        this.sortDesc = !this.sortDesc;\n      } else {\n        this.sortBy = column;\n        this.sortDesc = false;\n      }\n      this.sensors.sort((a, b) => {\n        let aValue = a[this.sortBy];\n        let bValue = b[this.sortBy];\n        if (typeof aValue === 'string' && typeof bValue === 'string') {\n          aValue = aValue.toLowerCase();\n          bValue = bValue.toLowerCase();\n        }\n        if (aValue === bValue) {\n          return 0;\n        } else if (aValue < bValue) {\n          return this.sortDesc ? 1 : -1;\n        } else {\n          return this.sortDesc ? -1 : 1;\n        }\n      });\n    }\n  },\n  computed: {\n    filteredSensors() {\n      const searchText = this.searchText.toLowerCase();\n      return this.sensors.filter(sensor => {\n        return sensor.name.toLowerCase().indexOf(searchText) !== -1;\n      });\n    }\n  },\n  mounted() {\n    this.loadSensors();\n  }\n};","map":{"version":3,"names":["axios","name","data","sensors","sortBy","sortDesc","searchText","showId","showName","showType","showVariant","methods","loadSensors","get","then","response","catch","error","console","log","sort","column","a","b","aValue","bValue","toLowerCase","computed","filteredSensors","filter","sensor","indexOf","mounted"],"sources":["src/views/Table.vue"],"sourcesContent":["<template>\r\n    <div>\r\n        <h2>Sensors</h2>\r\n        <div class=\"search-container\">\r\n            <input v-model=\"searchText\" type=\"text\" placeholder=\"Search by sensor name\">\r\n        </div>\r\n        <table>\r\n            <thead>\r\n                <tr>\r\n                    <th><input type=\"checkbox\" v-model=\"showId\">id</th>\r\n                    <th><input type=\"checkbox\" v-model=\"showName\">Name</th>\r\n                    <th><input type=\"checkbox\" v-model=\"showType\">Type</th>\r\n                    <th><input type=\"checkbox\" v-model=\"showVariant\">Variant</th>\r\n                    <th>Actions</th>\r\n                </tr>\r\n            </thead>\r\n            <tbody>\r\n                <tr v-for=\"(sensor, index) in filteredSensors\" :key=\"index\">\r\n                    <td v-if=\"showId\">{{ sensor.id }}</td>\r\n                    <td v-if=\"showName\">{{ sensor.name }}</td>\r\n                    <td v-if=\"showType\">{{ sensor.type }}</td>\r\n                    <td v-if=\"showVariant\">{{ sensor.variant }}</td>\r\n                    <td>\r\n                        <router-link :to=\"{ path: `/api/sensors/${sensor.id}/` }\" class=\"view-link\">\r\n                            View\r\n                        </router-link>\r\n                    </td>\r\n                </tr>\r\n            </tbody>\r\n        </table>\r\n    </div>\r\n</template>\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n    name: 'SensorList',\r\n    data() {\r\n        return {\r\n            sensors: [],\r\n            sortBy: null,\r\n            sortDesc: false,\r\n            searchText: '',\r\n            showId: true,\r\n            showName: true,\r\n            showType: true,\r\n            showVariant: true,\r\n        };\r\n    },\r\n    methods: {\r\n        loadSensors() {\r\n            axios\r\n                .get('http://localhost:8000/api/sensors/')\r\n                .then((response) => {\r\n                    this.sensors = response.data;\r\n                })\r\n                .catch((error) => {\r\n                    console.log(error);\r\n                });\r\n        },\r\n        sort(column) {\r\n            if (this.sortBy === column) {\r\n                this.sortDesc = !this.sortDesc;\r\n            } else {\r\n                this.sortBy = column;\r\n                this.sortDesc = false;\r\n            }\r\n            this.sensors.sort((a, b) => {\r\n                let aValue = a[this.sortBy];\r\n                let bValue = b[this.sortBy];\r\n\r\n                if (typeof aValue === 'string' && typeof bValue === 'string') {\r\n                    aValue = aValue.toLowerCase();\r\n                    bValue = bValue.toLowerCase();\r\n                }\r\n\r\n                if (aValue === bValue) {\r\n                    return 0;\r\n                } else if (aValue < bValue) {\r\n                    return this.sortDesc ? 1 : -1;\r\n                } else {\r\n                    return this.sortDesc ? -1 : 1;\r\n                }\r\n            });\r\n        },\r\n    },\r\n    computed: {\r\n        filteredSensors() {\r\n            const searchText = this.searchText.toLowerCase();\r\n            return this.sensors.filter((sensor) => {\r\n                return sensor.name.toLowerCase().indexOf(searchText) !== -1;\r\n            });\r\n        },\r\n    },\r\n    mounted() {\r\n        this.loadSensors();\r\n    },\r\n};\r\n</script>\r\n  \r\n\r\n<style scoped>\r\n/* Add your styles here */\r\nh2 {\r\n    font-size: 24px;\r\n    font-weight: bold;\r\n    margin-bottom: 16px;\r\n}\r\n\r\n.search-container {\r\n    margin-bottom: 16px;\r\n}\r\n\r\ntable {\r\n    width: 100%;\r\n    border-collapse: collapse;\r\n}\r\n\r\nth,\r\ntd {\r\n    padding: 8px;\r\n    text-align: left;\r\n    border: 1px solid #ccc;\r\n}\r\n\r\ntr:nth-child(even) {\r\n    background-color: #f9f9f9;\r\n}\r\n\r\ntr:hover {\r\n    background-color: #f2f2f2;\r\n}\r\n\r\ninput[type=\"text\"] {\r\n    padding: 8px;\r\n    border: 1px solid #ccc;\r\n    border-radius: 4px;\r\n    font-size: 13px;\r\n    width: 10%;\r\n    box-sizing: border-box;\r\n}\r\n\r\n.metric-columns-container {\r\n    display: flex;\r\n    flex-wrap: wrap;\r\n    margin-bottom: 16px;\r\n}\r\n\r\n.metric-column-checkbox {\r\n    margin-right: 8px;\r\n}\r\n\r\n.metric-column-label {\r\n    font-weight: bold;\r\n    margin-right: 8px;\r\n}\r\n\r\n.metric-column-checkbox:checked+.metric-column-label {\r\n    text-decoration: underline;\r\n}\r\n\r\n.metric-column-checkbox:checked+.metric-column-label::after {\r\n    content: \" (Shown)\";\r\n    font-weight: normal;\r\n    font-size: 12px;\r\n    color: #666;\r\n}\r\n\r\n.metric-column-checkbox:not(:checked)+.metric-column-label::after {\r\n    content: \" (Hidden)\";\r\n    font-weight: normal;\r\n    font-size: 12px;\r\n    color: #666;\r\n    text-decoration: line-through;\r\n}\r\n\r\n@media only screen and (max-width: 768px) {\r\n    .metric-column-checkbox {\r\n        margin-right: 0;\r\n        margin-bottom: 8px;\r\n    }\r\n}\r\n</style>\r\n"],"mappings":"AAiCA,OAAAA,KAAA;AAEA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,OAAA;MACAC,MAAA;MACAC,QAAA;MACAC,UAAA;MACAC,MAAA;MACAC,QAAA;MACAC,QAAA;MACAC,WAAA;IACA;EACA;EACAC,OAAA;IACAC,YAAA;MACAZ,KAAA,CACAa,GAAA,uCACAC,IAAA,CAAAC,QAAA;QACA,KAAAZ,OAAA,GAAAY,QAAA,CAAAb,IAAA;MACA,GACAc,KAAA,CAAAC,KAAA;QACAC,OAAA,CAAAC,GAAA,CAAAF,KAAA;MACA;IACA;IACAG,KAAAC,MAAA;MACA,SAAAjB,MAAA,KAAAiB,MAAA;QACA,KAAAhB,QAAA,SAAAA,QAAA;MACA;QACA,KAAAD,MAAA,GAAAiB,MAAA;QACA,KAAAhB,QAAA;MACA;MACA,KAAAF,OAAA,CAAAiB,IAAA,EAAAE,CAAA,EAAAC,CAAA;QACA,IAAAC,MAAA,GAAAF,CAAA,MAAAlB,MAAA;QACA,IAAAqB,MAAA,GAAAF,CAAA,MAAAnB,MAAA;QAEA,WAAAoB,MAAA,wBAAAC,MAAA;UACAD,MAAA,GAAAA,MAAA,CAAAE,WAAA;UACAD,MAAA,GAAAA,MAAA,CAAAC,WAAA;QACA;QAEA,IAAAF,MAAA,KAAAC,MAAA;UACA;QACA,WAAAD,MAAA,GAAAC,MAAA;UACA,YAAApB,QAAA;QACA;UACA,YAAAA,QAAA;QACA;MACA;IACA;EACA;EACAsB,QAAA;IACAC,gBAAA;MACA,MAAAtB,UAAA,QAAAA,UAAA,CAAAoB,WAAA;MACA,YAAAvB,OAAA,CAAA0B,MAAA,CAAAC,MAAA;QACA,OAAAA,MAAA,CAAA7B,IAAA,CAAAyB,WAAA,GAAAK,OAAA,